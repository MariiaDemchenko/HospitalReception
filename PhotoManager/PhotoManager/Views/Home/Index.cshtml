@{
    ViewBag.Title = "Home Page";
}
@model IEnumerable<PhotoManager.DAL.Models.Photo>

<div class="row">
    @foreach (var photo in Model)
    {
        <div class="col-lg-4">
            <img src="data:image;base64,@Convert.ToBase64String(photo.Image)" class="photo" />
        </div>
    }
</div>