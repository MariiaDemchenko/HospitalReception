@{
    ViewBag.Title = "Home Page";
}
@model IEnumerable<PhotoManager.DAL.Models.Photo>

<div class="row text-left">
    @foreach (var photo in Model)
    {
        <p class="col-lg-4">
            <img src="data:image;base64,@Convert.ToBase64String(photo.Image)" class="thumbnail" />
        </p>
    }
</div>