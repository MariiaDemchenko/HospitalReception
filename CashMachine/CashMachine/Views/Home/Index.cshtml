@using CashMachine.Common
@using CashMachine.ViewModels
@using (Html.BeginForm("CheckCardNum", "Home", FormMethod.Post, new { id = "needs-validation", novalidate = "", @class = "form-cash-machine" }))
{
    <div class="page-header">
        <h2>@Resources.InputCardNum</h2>
    </div>
    <div class="form-row">
        @Html.Partial("Numpad/_Numpad", new NumDisplayViewModel { Id = "inputCardNum", Mask = Constants.CardNumMask })
    </div>
}